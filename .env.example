# ===== AI PROVIDERS =====
# OpenRouter API (Default - supports GPT-4, Claude, Gemini, etc.)
OPENROUTER_API_KEY=your_openrouter_key_here
VITE_OPENROUTER_API_KEY=your_openrouter_key_here
OPENROUTER_DEFAULT_MODEL=meta-llama/llama-3.3-70b-instruct:free

# Gemini API (Fallback)
# VITE_GEMINI_API_KEY=your_gemini_key_here
# GEMINI_API_KEY=your_gemini_key_here

# Gemini Audio API
GEMINI_AUDIO_API_KEY=your_gemini_audio_key_here
VITE_GEMINI_AUDIO_API_KEY=your_gemini_audio_key_here

# AI Provider: "openrouter" or "gemini" (default: openrouter)
AI_PROVIDER=openrouter

# ===== SUPABASE =====
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# ===== SERVER =====
PORT=5000
VITE_API_URL=https://your-app.onrender.com/api

# ===== FIREBASE (Phone Auth) =====
VITE_FIREBASE_API_KEY=your_firebase_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_firebase_domain_here
VITE_FIREBASE_PROJECT_ID=your_firebase_project_id_here
VITE_FIREBASE_STORAGE_BUCKET=your_firebase_bucket_here
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_id_here
VITE_FIREBASE_APP_ID=your_app_id_here

# ===== RESEND (Email OTP) =====
RESEND_API_KEY=your_resend_key_here
